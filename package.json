    "steal-tools": "^2.0.0",
    "testee": "^0.9.0",
    "tmp": "^0.0.31"
  },
  "steal": {
    "directories": {
      "lib": "src"
    },
    "main": "syn",
    "map": {},
    "ignoreBrowser": true
  },
